    "@reduxjs/toolkit": "^1.9.5",
    "contentlayer": "^0.3.4",
    "dayjs": "^1.11.9",
    "gray-matter": "^4.0.3",
    "next": "13.4.12",
    "next-contentlayer": "^0.3.4",
    "next-mdx-remote": "^4.4.1",
    "next-sitemap": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-redux": "^8.1.2",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
